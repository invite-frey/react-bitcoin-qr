{
  "name": "react-bitcoin-qr",
  "version": "0.0.2",
  "description": "React component for a Bitcoin QR code. Supports optional attributes 'amount' and 'message' in addition to required attribute 'address'. Also acts as a link for browsers that support 'bitcoin:' format URIs.",
  "main": "index.js",
  "dependencies": {
    "bitcore-lib": "^0.13.13",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-qr": "^0.0.2"
  },
  "devDependencies": {
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "q": "^1.4.1",
    "simple-server": "^1.0.1"
  },
  "keywords": [
    "bitcoin",
    "qr",
    "address",
    "bitcore",
    "payments"
  ],
  "author": "Willy Bruns",
  "license": "MIT",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
